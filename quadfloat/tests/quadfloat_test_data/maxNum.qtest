operation: maxNum
operation source: binary16

# Simple cases.
0x1p0 0x3p0 -> 0x3p0
0x3p0 0x1p0 -> 0x3p0
0x1p0 -0x3p0 -> 0x1p0
-0x3p0 0x1p0 -> 0x1p0
-0x1p0 0x3p0 -> 0x3p0
0x3p0 -0x1p0 -> 0x3p0
-0x1p0 -0x3p0 -> -0x1p0
-0x3p0 -0x1p0 -> -0x1p0

# Equal values.
0x1p0 0x1p0 -> 0x1p0
-0x1p0 -0x1p0 -> -0x1p0
0x3p0 0x3p0 -> 0x3p0
-0x3p0 -0x3p0 -> -0x3p0

# Infinities
0x2p0 Infinity -> Infinity
0x2p0 -Infinity -> 0x2p0
Infinity -Infinity -> Infinity

# Zeros (implementation-defined behaviour
# when -0 and 0 are compared).
0x0p0 0x0p0 -> 0x0p0
0x0p0 -0x0p0 -> 0x0p0
-0x0p0 0x0p0 -> 0x0p0
-0x0p0 -0x0p0 -> -0x0p0

# Subnormals
0x1p-24 0x2p-24 -> 0x2p-24 underflow
-0x1p-24 0x2p-24 -> 0x2p-24 underflow
0x1p-24 -0x2p-24 -> 0x1p-24 underflow
-0x1p-24 -0x2p-24 -> -0x1p-24 underflow

# Finite number and quiet NaN.
0x1p0 NaN -> 0x1p0
NaN -0x5p-2 -> -0x5p-2
-NaN(123) NaN(456) -> -NaN(123)  # first nan chosen
NaN(456) -NaN(123) -> NaN(456)

# Finite number and signaling NaN.
0x1p0 sNaN(123) -> NaN(123) invalid
-sNaN(45) 0x1p0 -> -NaN(45) invalid

# NaN mixtures.
NaN(123) NaN(2) -> NaN(123)
sNaN(123) NaN(2) -> NaN(123) invalid
NaN(123) sNaN(2) -> NaN(2) invalid
sNaN(123) sNaN(2) -> NaN(123) invalid
