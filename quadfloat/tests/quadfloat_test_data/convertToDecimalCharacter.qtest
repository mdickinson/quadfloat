# Possible to give a negative number to round to
# the nearest hundred, thousand, ...
-0x0p0 ".-2f" -> "-0"
0x0p0 ".-2f" -> "0"
0xap0 ".-2f" -> "0"
0x32p0 ".-2f" -> "0"
0x33p0 ".-2f" -> "100"
0x40p0 ".-2f" -> "100"
0x95p0 ".-2f" -> "100"
0x96p0 ".-2f" -> "200"
0x1p8 ".-2f" -> "300"
0x0.ffep16 ".-2f" -> "65500"

# Infinities
-Infinity ".6f" -> "-Infinity"
Infinity ".6f" -> "Infinity"

# NaNs
NaN ".6f" -> "NaN"
-NaN ".6f" -> "-NaN"
sNaN ".6f" -> "sNaN"
